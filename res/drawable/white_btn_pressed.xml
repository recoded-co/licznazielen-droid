<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape
            android:shape="rectangle" >
            <stroke android:width="1dp" android:color="@color/whitecolor" />
            <solid android:color="@color/whitecolor" />
        </shape>
    </item>
    <item android:left="40dp" android:right="40dp">
        <shape
            android:shape="rectangle" >
            <stroke android:width="1dp" android:color="@color/normal_btn" />
            <solid android:color="@color/normal_btn" />
        </shape>
    </item>

    <item android:bottom="1dp" >
        <shape
            android:shape="rectangle">
            <stroke android:width="1dp" android:color="@color/line_cancel" />
            <solid android:color="@color/line_cancel" />
        </shape>
    </item>
</layer-list>